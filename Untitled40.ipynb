    "  Array.new(height){Array.new(width,value)}\n",
    "end\n",
    "\n",
    "award=[[2,6,5,-2],[3,-1,4,-3],[-3,12,-1,8]]\n",
    "\n",
    "b=award\n",
    "m=b.length()\n",
    "n=b[0].length()\n",
    "a=make2d(m,n,0)\n",
    "\n",
    "def max(x,y)\n",
    "  if x<y\n",
    "    y\n",
    "  else\n",
    "    x\n",
    "  end\n",
    "end"
